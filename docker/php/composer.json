{
    "name": "ecreos/invogue",
    "type": "project",
    "description": "Ecreos based site #Invogue",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "darkaonline/l5-swagger": "5.8.*",
        "darryldecode/cart": "4.2.0",
        "fruitcake/laravel-cors": "^1.0",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "jenssegers/mongodb": "^3.5.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.6",
        "ongr/elasticsearch-dsl": "^7.0",
        "php-http/curl-client": "^1.7",
        "phpoffice/phpspreadsheet": "^1.6",
        "predis/predis": "1.1.1",
        "spatie/image": "1.*",
        "spatie/laravel-missing-page-redirector": "^2.7",
        "spatie/laravel-responsecache": "6"
    },
    "require-dev": {
        "ecreos/1c-commerceml2": "dev-master",
        "ecreos/actions-logger": "dev-master",
        "ecreos/banners": "dev-master",
        "ecreos/core": "dev-master",
        "ecreos/customer-cabinet": "dev-master",
        "ecreos/customer-phone-verification": "dev-master",
        "ecreos/esputnik": "dev-master",
        "ecreos/facebook-feed": "dev-master",
        "ecreos/google-feed": "dev-master",
        "ecreos/liqpay-payment-method": "dev-master",
        "ecreos/looks": "dev-master",
        "ecreos/novaposhta-delivery": "dev-master",
        "ecreos/shops": "dev-master",
        "barryvdh/laravel-debugbar": "3.3.3",
        "barryvdh/laravel-ide-helper": "2.6.7",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "exclude-from-classmap": ["/tests/"],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/factories",
            "database/seeds"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./core/"
        },
        {
            "type": "path",
            "url": "./modules/ActionsLogger/"
        },
        {
            "type": "path",
            "url": "./modules/OneC_CommerceML2/"
        },
        {
            "type": "path",
            "url": "./modules/Shops/"
        },
        {
            "type": "path",
            "url": "./modules/Esputnik/"
        },
        {
            "type": "path",
            "url": "./modules/Looks/"
        },
        {
            "type": "path",
            "url": "./modules/CustomerPhoneVerification/"
        },
        {
            "type": "path",
            "url": "./modules/CustomerCabinet/"
        },
        {
            "type": "path",
            "url": "./modules/Banners/"
        },
        {
            "type": "path",
            "url": "./modules/Liqpay/"
        },
        {
            "type": "path",
            "url": "./modules/Novaposhta/"
        },
        {
            "type": "path",
            "url": "./modules/GoogleFeed/"
        },
        {
            "type": "path",
            "url": "./modules/FacebookFeed/"
        }

    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
